<!DOCTYPE HTML>
<html lang="fr">
    <head>
        <link href="{{ app.request.basepath }}/css/style.css" type="text/css" rel="stylesheet" />
        <link href="{{ app.request.basepath }}/css/style-xml.css" type="text/css" rel="stylesheet" />
        <link href="{{ app.request.basepath }}/css/prettify.css" type="text/css" rel="stylesheet" />
        <title>{% block title %}{{ app.config.title }}{% endblock %}</title>
    </head>
    <body onload="prettyPrint()">
        {% block menu %}
            {% include 'html/menu.html.twig' %}
        {% endblock %}
        <div id="content">
            {% block content %}{% endblock %}
        </div>
        <script type="text/javascript" src="{{ app.request.basepath }}/js/script.js"></script>
        <script type="text/javascript" src="{{ app.request.basepath }}/js/prettify/prettify.js"></script>
    </body>
</html>


